# Open WebUI Configuration
# Copy this file to .env and fill in your values

# Open WebUI URL (required)
# Default: http://localhost:8080
OPENWEBUI_URL=http://localhost:8080

# Open WebUI API Key (required)
# Get from: Open WebUI → Settings → Account → API Key
OPENWEBUI_API_KEY=sk-your-api-key-here

# Optional: File extensions to import (comma-separated)
# Example: .pdf,.txt,.md,.doc,.docx
# Default: all files
# OPENWEBUI_EXTENSIONS=

# Optional: Enable verbose logging
# Default: false
# OPENWEBUI_VERBOSE=true
